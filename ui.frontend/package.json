  "scripts": {
    "start": "cross-env NODE_ENV=development craco start",
    "build": "craco build && clientlib",
    "build:dev": "cross-env NODE_ENV=development craco build && clientlib",
    "build-with-ssr": "cross-env NODE_ENV=production craco build && webpack --config webpack.config.adobeio.js && clientlib",
    "build-with-express": "cross-env NODE_ENV=production craco build && webpack --config webpack.config.express.js && clientlib",
    "start-ssr-ioruntime": "cross-env NODE_ENV=development aio app run --local",
    "start-ssr-express": "node dist/express.js",
    "deploy-ssr-ioruntime": "cross-env NODE_ENV=production aio app deploy",
    "test": "craco test",
    "eject": "craco eject",
    "sync": "aemsync -d -w ../ui.apps/src/main/content",
    "component": "dept-react-scripts component",
    "aem-component": "node aem-scripts/aem_generate_component.js",
    "svgr": "npx @svgr/cli@5 ./src/assets/svg --template ./src/assets/svg/svgrtemplate.js --ext tsx --out-dir ./src/assets/svg/components --ref",
    "postinstall": "patch-package",
    "pauto-install": "cd ../ && mvn clean install -PautoInstallSinglePackage",
    "pauto-install-dev": "cd ../ && mvn clean install -PautoInstallSinglePackage -DNODE_ENV=development",
    "type-check": "tsc",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
